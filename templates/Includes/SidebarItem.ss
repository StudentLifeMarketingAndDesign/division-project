<div class="mod <% if $Image %>photo<% end_if %>">
<% if $ShowHeading %> 
    <div>
    	<% if $Link %>
			<a href="$Link">
				
		        <h3>$Title</h3>
		    
		        <% if $Image %>
		        	<img src="data:image/png;base64," data-src="$Image.ScaleWidth(279).URL" role="presentation" alt="">
		        <% end_if %>
	    	</a>
    	<% else %>
		
	        <h3>$Title</h3>
	        
	        <% if $Image %>
	        	<img src="data:image/png;base64," data-src="$Image.URL" role="presentation" alt="">
	        <% end_if %>
    	<% end_if %>
    </div>
<% end_if %>
    
    <% if RSSFeedLink %>
    	<% include SidebarRssFeed %>
    <% end_if %>
    $Content
</div>
